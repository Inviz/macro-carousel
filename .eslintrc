{
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "extends": ["google"],
  "rules": {
    "max-len": ["error", {"ignoreComments": true}],
    "arrow-parens": 0,
    "valid-jsdoc": "off",
    "require-jsdoc": 0,
    "keyword-spacing": ["error"],
    "no-console": ["error", {"allow": ["warn", "error"]}],
    "no-extra-semi": ["error"],
    "nonblock-statement-body-position": ["error", "below"],
    "no-invalid-this": "off",
    "func-style": "off"
  }
}
